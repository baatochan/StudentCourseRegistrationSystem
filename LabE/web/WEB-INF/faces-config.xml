<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
    http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <converter>
        <converter-for-class>testApp.LokalizacjaEntity</converter-for-class>
        <converter-class>testApp.LokalizacjaEntityConverter</converter-class>
    </converter>
    <converter>
        <converter-for-class>testApp.OsobyEntity</converter-for-class>
        <converter-class>testApp.OsobyEntityConverter</converter-class>
    </converter>
    <converter>
        <converter-for-class>testApp.SprzetEntity</converter-for-class>
        <converter-class>testApp.SprzetEntityConverter</converter-class>
    </converter>
    <managed-bean>
        <managed-bean-name>lokalizacjaEntity</managed-bean-name>
        <managed-bean-class>testApp.LokalizacjaEntityManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>osobyEntity</managed-bean-name>
        <managed-bean-class>testApp.OsobyEntityManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sprzetEntity</managed-bean-name>
        <managed-bean-class>testApp.SprzetEntityManagedBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>

    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>editLokalizacjaEntity</from-outcome>
            <to-view-id>/lokalizacjaEntity/EditLokalizacjaEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewLokalizacjaEntity</from-outcome>
            <to-view-id>/lokalizacjaEntity/ViewLokalizacjaEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>lokalizacjaEntityList</from-outcome>
            <to-view-id>/lokalizacjaEntity/ListLokalizacjaEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createLokalizacjaEntity</from-outcome>
            <to-view-id>/lokalizacjaEntity/CreateLokalizacjaEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editOsobyEntity</from-outcome>
            <to-view-id>/osobyEntity/EditOsobyEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewOsobyEntity</from-outcome>
            <to-view-id>/osobyEntity/ViewOsobyEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>osobyEntityList</from-outcome>
            <to-view-id>/osobyEntity/ListOsobyEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createOsobyEntity</from-outcome>
            <to-view-id>/osobyEntity/CreateOsobyEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>editSprzetEntity</from-outcome>
            <to-view-id>/sprzetEntity/EditSprzetEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>viewSprzetEntity</from-outcome>
            <to-view-id>/sprzetEntity/ViewSprzetEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>sprzetEntityList</from-outcome>
            <to-view-id>/sprzetEntity/ListSprzetEntity.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>createSprzetEntity</from-outcome>
            <to-view-id>/sprzetEntity/CreateSprzetEntity.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>